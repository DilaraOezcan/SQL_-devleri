1) SELECT DISTINCT(rating),title FROM film GROUP BY title,rating ORDER BY rating

2) SELECT replacement_cost, COUNT(replacement_cost) FROM film GROUP BY replacement_cost HAVING COUNT(replacement_cost) > 50 ;

3) SELECT store_id, COUNT(customer) FROM customer GROUP BY store_id;

4) SELECT MAX(country_id),COUNT(city) FROM city GROUP BY country_id ORDER BY COUNT(city) DESC LIMIT 1;
